{
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "react-native"
  ],
  "extends": [
    "google",
    "plugin:react/recommended"
  ],
  "rules": {
    "require-jsdoc": 0,
    "max-len": 1,
    "arrow-parens": 0,
    "linebreak-style": 0,
    "object-curly-spacing": 0
  }
}